---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
   <url>
      <loc>{{ site.url }}/about_me.html</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>1</priority>
   </url>
   <url>
      <loc>{{ site.url }}/contact.html</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
   <url>
      <loc>{{ site.url }}/clients.html</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
   <url>
      <loc>{{ site.url }}/modus_operandi.html</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
   <url>
      <loc>{{ site.url }}/bits.html</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
{% for post in site.posts %}
   <url>
      <loc>{{ site.url }}{{ page.rooturl }}{{ post.url }}</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.8</priority>
   </url>
{% endfor %}
</urlset>
